MLIDbrowser { // MultiLevelIdentityDictionary	var <>onSelect;		*new { arg browseMe,  onSelect;		^super.new			.onSelect_(onSelect ? { arg item;  item.gui })			.browse(browseMe ?? {Library.global.dictionary})	}		browse { arg item,parent;		if(item.isKindOf(IdentityDictionary).not,{			onSelect.value(item);		},{			CXMenu.newWith(				item.keys.asList.collect({ arg key;					key->{ this.browse(item.at(key),item) }				})			)			.gui		})	}}