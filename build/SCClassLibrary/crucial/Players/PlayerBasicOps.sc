PlayerUnop : AbstractPlayer {	var <>operator, <>a;		*new { arg operator, a;		^super.new.a_(loadPath(a)).operator_(operator)	}	ar {		^a.value.perform(operator)	}	storeParamsOn { arg stream;		stream.storeArgs([operator,enpath(a)])	}	guiClass { ^PlayerUnopGui }}PlayerBinop : AbstractPlayer{	var <>operator, <>a, <>b;	*new { arg operator, a, b;		^super.new.a_(loadPath(a)).b_(loadPath(b)).operator_(operator)	}	ar {		^a.value.perform(operator,b.value)	}	storeParamsOn { arg stream;		stream.storeArgs([operator,enpath(a),enpath(b)])	}	guiClass { ^PlayerBinopGui }}