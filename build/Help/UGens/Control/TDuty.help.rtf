(\
	\{\
		\cf3 var\cf0  a, trig, n=5, m=64;\
		a = \{\
			\cf3 var\cf0  x;\
			x = \{ 0.2.rand2 \} ! m;\
			x = x ++ (\{  \cf3 Drand\cf0 (\{ 0.2.rand2 \} ! n) \} ! m.rand);\
			\cf3 Dseq\cf0 (x.scramble, \cf3 inf\cf0 )\
