Interval : Collection {	var <>start, <>end, <>step;		*new { arg start, end, step=1;		^super.new.start_(start).end_(end).step_(step)	}		size { ^end - start div: step + 1 }	at { arg index; 		var val;		if (index < 0 or: { index >= this.size }, { ^nil });		^step * index + start;	}	do { arg function;		forBy(start, end, step, function);	}		add { ^this.shouldNotImplement }	put { ^this.shouldNotImplement }		storeParamsOn { arg stream;		stream.putAll("( ");		[start, end, step].storeItemsOn(stream);		stream.putAll(" )");	}}